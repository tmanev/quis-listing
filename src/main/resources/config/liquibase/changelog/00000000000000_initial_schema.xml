            </column>
            <column name="has_search_configuration" type="tinyint(1)">
                <constraints nullable="false"/>
            </column>
            <column name="can_be_ordered" type="tinyint(1)">
                <constraints nullable="false"/>
            </column>
            <column name="hide_name" type="tinyint(1)">
                <constraints nullable="false"/>
            </column>
            <column name="on_excerpt_page" type="tinyint(1)">
